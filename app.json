{
  "expo": {
    "name": "One Step",
    "slug": "one-step-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logoOneStep.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/logoOneStep.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "buildNumber": "1.0.0",
      "supportsTablet": true,
      "bundleIdentifier": "com.martinfrangul.onestepapp",
      "infoPlist": {
        "UIBackgroundModes": [
          "audio",
          "fetch"
        ],
        "NSUserNotificationUsageDescription": "This app needs access to show notifications."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logoOneStep.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.martinfrangul.onestepapp"
    },
    "web": {
      "favicon": "./assets/images/browsericon.png"
    },
    "plugins": [
      "expo-font"
    ],
    "extra": {
      "eas": {
        "projectId": "6f35e276-8c5b-4a1b-a3bb-fa8bb828bf11"
      }
    },
    "owner": "borisfindell"
  }
}
